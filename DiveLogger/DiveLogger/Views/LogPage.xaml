<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DiveLogger.Views.LogPage"
             xmlns:pages="clr-namespace:DiveLogger.Views"
                  Icon="iconLog.png"
                  >
  <MasterDetailPage.Master >
    <pages:LogPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail >
     
    <NavigationPage Style="{StaticResource NavPage}">
      <x:Arguments>
        <pages:DiveLogPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>